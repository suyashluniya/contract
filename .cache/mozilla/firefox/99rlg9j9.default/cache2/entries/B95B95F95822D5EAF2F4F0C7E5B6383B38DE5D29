 -->\n    <pagination ng-show="issues !== false && total_items > 30"\n                on-select-page="getIssues(page)"\n                page="page"\n                items-per-page="per_page"\n                max-size="10"\n                total-items="total_items"\n                rotate="false"\n                boundary-links="true">\n    </pagination>\n  </div>\n</div>')}]),angular.module("app").controller("TeamIssuesController",["$scope","$api","$routeParams","$location","$anchorScroll","feature",function($scope,$api,$routeParams,$location,$anchorScroll,feature){function initializeTrackersFromParams(params){if(params.tracker_ids){for(var tracker_id_array=params.tracker_ids.split(","),new_selected_trackers={},i=0;i<tracker_id_array.length;i++)new_selected_trackers[tracker_id_array[i]]=!0;return new_selected_trackers}}function setPagination(pagination_data){$scope.page=pagination_data.page,$scope.total_items=parseInt(pagination_data.total_items,10),$scope.maxSize=10,$scope.per_page=pagination_data.per_page}function getTrackerIds(){for(var processed_tracker_ids=[],tracker_ids=Object.keys($scope.selected_trackers),i=0;i<tracker_ids.length;i++)$scope.selected_trackers[tracker_ids[i]]&&processed_tracker_ids.push(tracker_ids[i]);return processed_tracker_ids.length>0&&updateRouteParams({tracker_ids:processed_tracker_ids.join(",")}),processed_tracker_ids}function updateRouteParams(options){$location.search(options)}function createIssueStatusParam(issue_status){return"open"===issue_status?!0:"closed"===issue_status?!1:null}function createPaidStatusParam(issue_status){return"paid_out"===issue_status?!0:null}$scope.issue_grid_view=!1,$scope.rfpEnabled=feature.enabled("rfp"),$scope.selected_trackers=initializeTrackersFromParams($routeParams)||{},$scope.team_promise.then(function(team){$scope.getIssues=function(page){if($scope.issues=!1,$scope.bounty_max<$scope.bounty_min)return void($scope.show_bounty_error=!0);$scope.show_bounty_error=!1;var selected_tracker_ids=getTrackerId
 <textarea class='form-control' ng-model='developer_form.data.note' placeholder='Provide some details for the backers!' rows='4'></textarea>\n          </div>\n        </div>\n        <div class='form-group'>\n          <div class='col-sm-offset-2 col-sm-10'>\n            <button class='btn btn-primary' ng-show=\"['has_solution','has_goal','has_stopped_solution','claim_submitted','claim_needed_no_solution','claim_needed_has_solution'].indexOf(developer_form.data.status)&gt;=0\" type='submit'>Save</button>\n            <a class='btn btn-default' ng-click='developer_form.cancel()'>Cancel</a>\n            <a class='btn btn-danger' ng-click='developer_form.delete_bounty_claim(my_bounty_claim)' ng-show=\"developer_form.data.status=='claim_submitted'\">\n              <span class='glyphicon glyphicon-trash'></span>\n              Retract Claim\n            </a>\n            <a class='btn btn-danger' ng-click='developer_form.delete_bounty_goal()' ng-if=\"developer_form.data.status=='has_goal' &amp;&amp; my_developer_goal\">Delete</a>\n          </div>\n        </div>\n      </form>\n    </div>\n    <table class='table table-developer-statuses' ng-show='developer_events.length&gt;0' style='table-layout:fixed; margin-bottom: 0; border-top: 0'>\n      <tr ng-class=\"{ 'row-green': event.type=='bounty_claim' &amp;&amp; event.collected }\" ng-repeat=\"event in developer_events | orderBy:['-updated_at']\">\n        <!-- 50px profile pic for mobile, 100px for desktop -->\n        <td class='hidden-xs hidden-sm' style='width:100px; vertical-align: top'>\n          <profile-pic owner='event.owner'></profile-pic>\n        </td>\n        <td class='hidden-md hidden-lg' style='width:50px; vertical-align: top'>\n          <profile-pic owner='event.owner'></profile-pic>\n        </td>\n        <td>\n          <div ng-if=\"event.type=='issue_suggestion'\">\n            <h4 style='margin-top: 0'>\n              <profile-name owner='event.owner'></profile-name>\n              <span ng-show='!event.thanked_at'>suggested this issue to</span>\n              <span ng-show='event.thanked_at &amp;&amp; !event.thanked_reward'>was thanked for suggesting this issue to</span>\n              <span ng-show='event.thanked_at &amp;&amp; event.thanked_reward'>was thanked and rewarded {{ event.thanked_reward | dollars}} for suggesting this issue to</span>\n              <a ng-href='/teams/{{event.team.slug}}'>{{event.team.display_name}}</a>.\n              <small>{{event.created_at | timeDiff }} ago</small>\n            </h4>\n            <p style='word-wrap:break-word;'>{{event.description}}</p>\n            <p class='text-danger' ng-show='event.rejected_at'>\n              <span class='glyphicon glyphicon-thumbs-down'></span>\n              <span ng-show='event.rejection_reason' style='word-wrap:break-word;'>Rejected {{event.rejected_at | timeDiff }} ago: {{ event.rejection_reason }}</span>\n              <span ng-hide='event.rejection_reason' style='word-wrap:break-word;'>Rejected {{event.rejected_at | timeDiff }} ago.</span>\n            </p>\n            <p ng-hide='!event.can_respond || event.show_accept_form || event.show_reject_form'>\n              <a class='btn btn-success' ng-click='event.show_accept_form=true; event.my_form_data={}'>\n                <span class='glyphicon glyphicon-thumbs-up'></span>\n                Thank\n              </a>\n              <a class='btn btn-danger' ng-click='event.show_reject_form=true; event.my_form_data={}'>\n                <span class='glyphicon glyphicon-thumbs-down'></span>\n                Reject\n              </a>\n            </p>\n            <form class='form-horizontal' ng-show='event.show_accept_form' ng-submit='developer_form.accept_issue_suggestion(event)' style='margin-top:20px'>\n              <div class='form-group'>\n                <label class='col-sm-2 control-label'>\n                  Reward:\n                </label>\n                <div class='col-sm-3'>\n                  <div class='input-group'>\n                    <div class='input-group-addon'>$</div>\n                    <input class='form-control' ng-model='event.my_form_data.thanked_reward' type='text'>\n                  </div>\n                </div>\n                <div class='col-sm-7 help-block'>\n                  Optionally reward\n                  <profile-name owner='event.owner'></profile-name>\n                  for suggesting this issue.\n                </div>\n              </div>\n              <div class='form-group' ng-show='issue.can_add_bounty'>\n                <label class='col-sm-2 control-label'>\n                  Bounty:\n                </label>\n                <div class='col-sm-3'>\n                  <div class='input-group'>\n                    <div class='input-group-addon'>$</div>\n                    <input class='form-control' ng-model='event.my_form_data.bounty_amount' type='text'>\n                  </div>\n                </div>\n                <div class='col-sm-7 help-block'>\n                  Optionally post a bounty on this issue.\n                </div>\n              </div>\n              <div class='form-group'>\n                <div class='col-sm-offset-2 col-sm-10'>\n                  <button class='btn btn-success'>\n                    <span ng-show='event.my_form_data.bounty_amount || event.my_form_data.thanked_reward'>\n                      <span class='glyphicon glyphicon-shopping-cart'></span>\n                      Continue to Cart\n                    </span>\n                    <span ng-hide='event.my_form_data.bounty_amount || event.my_form_data.thanked_reward'>\n                      <span class='glyphicon glyphicon-thumbs-up'></span>\n                      Confirm Thanks\n                    </span>\n                  </button>\n                  <a class='btn btn-default' ng-click='event.show_accept_form=false'>Cancel</a>\n                </div>\n              </div>\n            </form>\n            <form ng-show='event.show_reject_form' ng-submit='developer_form.reject_issue_suggestion(event)'>\n              <div class='form-group'>\n                <textarea class='form-control' ng-model='event.my_form_data.rejection_reason' placeholder='Why are you rejecting this suggestion? (Optional)' rows='4'></textarea>\n              </div>\n              <div class='form-group'>\n                <button class='btn btn-danger'>\n                  <span class='glyphicon glyphicon-thumbs-down'></span>\n                  Confirm Rejection\n                </button>\n                <a class='btn btn-default' ng-click='event.show_reject_form=false'>Cancel</a>\n              </div>\n            </form>\n          </div>\n          <div ng-if=\"event.type=='developer_goal'\">\n            <h4 style='margin-top: 0'>\n              <profile-name owner='event.owner'></profile-name>\n              set a goal of {{event.amount | dollars}}.\n              <small>{{event.updated_at | timeDiff }} ago</small>\n            </h4>\n            <div class='issue-goals progress progress-striped'>\n              <div class='progress-bar progress-bar-success' style='width: {{ 100*issue.bounty_total/event.amount | atLeast:15 | atMost:100 | number:0 }}%;'>\n                <span>{{issue.bounty_total/event.amount | percent}}</span>\n              </div>\n            </div>\n            <p>\n              <a class='btn btn-success' ng-click='backer_form.post_bounty(event.amount-issue.bounty_total)' ng-if='event.amount &gt; issue.bounty_total'>Post Bounty</a>\n              <a class='btn btn-default' ng-click='backer_form.post_bounty()' ng-if='event.amount &lt;= issue.bounty_total'>Increase Bounty</a>\n            </p>\n          </div>\n          <div ng-if=\"event.type=='solution'\">\n            <h4 style='margin-top: 0'>\n              <div ng-if=\"event.status=='started'\">\n                <profile-name owner='event.owner'></profile-name>\n                <span ng-if='!event.completion_date'>started working on this issue.</span>\n                <span ng-if='event.completion_date'>started working on this issue and expects to finish by {{event.completion_date | date}}.</span>\n                <small>{{event.updated_at | timeDiff }} ago</small>\n              </div>\n              <div ng-if=\"event.status=='stopped'\">\n                <profile-name owner='event.owner'></profile-name>\n                stopped working on this issue.\n                <small>{{event.updated_at | timeDiff }} ago</small>\n              </div>\n            </h4>\n            <p ng-if='event.note'>\n              <span class='text-muted'>\n                <profile-name owner='event.owner'></profile-name>\n                wrote:\n              </span>\n              {{event.note}}\n            </p>\n            <p ng-show='event.url'>\n              <span ng-if=\"event.url.indexOf('http://') != 0 &amp;&amp; event.url.indexOf('https://') != 0\">\n                Code URL: {{event.url}}\n              </span>\n              <span ng-if=\"event.url.indexOf('http://') == 0 || event.url.indexOf('https://') == 0\">\n                <a class='btn btn-default' ng-href='{{event.url}}' target='_blank'>\n                  <span class='glyphicon glyphicon-new-window'></span>\n                  View Code\n                </a>\n              </span>\n            </p>\n          </div>\n          <div ng-if=\"event.type=='bounty_claim'\">\n            <h4 style='margin-top: 0'>\n              <span ng-if='issue.paid_out'>\n                <span ng-if='event.collected'>\n                  <profile-name owner='event.owner'></profile-name>\n                  was awarded the\n                  <span style='font-size:24px'>{{event.amount | dollars}}</span>\n                  bounty!\n                </span>\n                <span ng-if='!event.collected'>\n                  <profile-name owner='event.owner'></profile-name>\n                  submitted a claim which wasn't accepted.\n                </span>\n              </span>\n              <span ng-if='!issue.paid_out'>\n                <span ng-if='event.contested'>\n                  <profile-name owner='event.owner'></profile-name>\n                  submitted a claim which is being contested\n                  <span ng-if='event.backers_count===1'>\n                    but the backer hasn't responded.\n                  </span>\n                  <span ng-if='event.backers_count&gt;1'>\n                    and {{event.accept_count+event.reject_count}} of {{event.backers_count}} backers have responded.\n                  </span>\n                </span>\n                <span ng-if='!event.contested &amp;&amp; !event.collected &amp;&amp; !event.disputed'>\n                  <profile-name owner='event.owner'></profile-name>\n                  submitted a claim\n                  <span ng-if='event.backers_count===1'>\n                    but the backer hasn't responded.\n                  </span>\n                  <span ng-if='event.backers_count&gt;1'>\n                    and {{event.accept_count}} of {{event.backers_count}} backers have responded.\n                  </span>\n                </span>\n                <span ng-if='!event.contested &amp;&amp; !event.collected &amp;&amp; event.disputed'>\n                  <profile-name owner='event.owner'></profile-name>\n                  submitted a claim which is being disputed.\n                </span>\n              </span>\n              <small>{{event.created_at | timeDiff }} ago</small>\n            </h4>\n            <p class='text-muted' ng-if='!issue.paid_out &amp;&amp; !event.contested &amp;&amp; !event.disputed' style='font-style: italic;'>\n              NOTE: This will be paid out if all backers accept this or if no backers reject this by {{event.dispute_period_ends_at | date:'MMM d, y h:mm a':'UTC' }} (GMT).\n            </p>\n            <p ng-if='event.description'>\n              <span class='text-muted'>\n                <profile-name owner='event.owner'></profile-name>\n                wrote:\n              </span>\n              {{event.description}}\n            </p>\n            <div>\n              <span ng-show='event.code_url'>\n                <span ng-if=\"event.code_url.indexOf('http://') != 0 &amp;&amp; event.code_url.indexOf('https://') != 0\">\n                  Code URL: {{event.code_url}}\n                </span>\n                <span ng-if=\"event.code_url.indexOf('http://') == 0 || event.code_url.indexOf('https://') == 0\">\n                  <a class='btn btn-default' ng-href='{{event.code_url}}' target='_blank'>\n                    <span class='glyphicon glyphicon-new-window'></span>\n                    View Code\n                  </a>\n                </span>\n              </span>\n            </div>\n            <div class='alert' ng-class=\"{'alert-info': event.my_response.value !== false, 'alert-danger': event.my_response.value === false }\" ng-if='issue.can_respond_to_claims &amp;&amp; event.in_dispute_period' style='margin-top:14px'>\n              <div ng-if='!event.my_form_data'>\n                <div ng-if='event.my_response.value!==true &amp;&amp; event.my_response.value!==false'>\n                  {{current_person.display_name}}, your response is needed:\n                  <a class='btn btn-success' ng-click=\"backer_form.start_response(event, 'accept')\">\n                    <span class='glyphicon glyphicon-thumbs-up'></span>\n                    Accept\n                  </a>\n                  <a class='btn btn-danger' ng-click=\"backer_form.start_response(event, 'reject')\">\n                    <span class='glyphicon glyphicon-thumbs-down'></span>\n                    Reject\n                  </a>\n                </div>\n                <div ng-if='event.my_response.value===true'>\n                  You approved this bounty claim {{ event.my_response.updated_at | timeDiff }} ago.\n                  <a class='btn btn-default' ng-click=\"backer_form.start_response(event, 'retract')\">\n                    Retract Vote\n                  </a>\n                </div>\n                <div ng-if='event.my_response.value===false'>\n                  You rejected this bounty claim {{ event.my_response.updated_at | timeDiff }} ago.\n                  <a class='btn btn-success' ng-click=\"backer_form.start_response(event, 'resolve')\">\n                    <span class='glyphicon glyphicon-thumbs-up'></span>\n                    Accept\n                  </a>\n                  <a class='btn btn-default' ng-click=\"backer_form.start_response(event, 'retract')\">\n                    Retract Vote\n                  </a>\n                </div>\n              </div>\n              <form ng-if='event.my_form_data &amp;&amp; event.my_form_data.action'>\n                <div class='form-group'>\n                  <textarea class='form-control' ng-model='event.my_form_data.description' placeholder='Please leave some feedback for the developer.' rows='4'></textarea>\n                </div>\n                <a class='btn btn-success' ng-click='backer_form.accept_claim(event)' ng-show=\"event.my_form_data.action==='accept'\">\n                  <span class='glyphicon glyphicon-thumbs-up'></span>\n                  Confirm Accept\n                </a>\n                <a class='btn btn-success' ng-click='backer_form.resolve_claim(event)' ng-show=\"event.my_form_data.action==='resolve'\">\n                  <span class='glyphicon glyphicon-thumbs-up'></span>\n                  Confirm Accept\n                </a>\n                <a class='btn btn-danger' ng-click='backer_form.reject_claim(event)' ng-show=\"event.my_form_data.action==='reject'\">\n                  <span class='glyphicon glyphicon-thumbs-down'></span>\n                  Confirm Reject\n                </a>\n                <a class='btn btn-primary' ng-click='backer_form.reset_claim(event)' ng-show=\"event.my_form_data.action==='retract'\">\n                  <span class='glyphicon glyphicon-ban-circle'></span>\n                  Retract Vote\n                </a>\n                <a class='btn btn-default' ng-click='backer_form.cancel_response(event)'>\n                  <span class='glyphicon glyphicon-thumbs-up'></span>\n                  Cancel\n                </a>\n              </form>\n            </div>\n            <table style='margin-top: 10px'>\n              <tr ng-repeat='response in event.responses'>\n                <td style='width:24px; vertical-align: top; padding-bottom: 5px'>\n                  <profile-pic owner='response.owner'></profile-pic>\n                </td>\n                <td style='vertical-align: top; padding-left: 5px'>\n                  <span ng-if='response.description'>\n                    <profile-name owner='response.owner'></profile-name>\n                    <span ng-switch='response.value'><span class='text-success' ng-switch-when='true'>accepted</span><span class='text-danger' ng-switch-when='false'>rejected</span><span class='text-muted' ng-switch-when='null'>retracted</span></span>:\n                  </span>\n                  <span ng-if='!response.description'>\n                    <profile-name owner='response.owner'></profile-name>\n                    <span ng-switch='response.value'><span class='text-success' ng-switch-when='true'>accepted</span><span class='text-danger' ng-switch-when='false'>rejected</span><span class='text-muted' ng-switch-when='null'>retracted</span></span>.\n                  </span>\n                  {{response.description}}\n                </td>\n              </tr>\n            </table>\n          </div>\n        </td>\n      </tr>\n    </table>\n  </div>\n</div>")
}]),angular.module("templates").run(["$templateCache",function($templateCache){$templateCache.put("app/issues/backers.html","<div ng-if='issue.id'>\n  <team-view>\n    <div ng-show='bounties.$resolved'>\n      <div class='alert alert-info' ng-hide='bounties.length &gt; 0'>\n        No bounties have been placed.\n      </div>\n      <div ng-show='bounties.length &gt; 0'>\n        <table class='table table-striped' id='bounties-table'>\n          <thead>\n            <tr>\n              <th>\n                <a ng-click=\"sort_by('owner.display_name')\">Backer</a>\n              </th>\n              <th></th>\n              <th>\n                <a ng-click=\"sort_by('amount')\">Amount</a>\n              </th>\n              <th>\n                <a ng-click=\"sort_by('created_at')\">Date</a>\n              </th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr ng-repeat='bounty in bounties | orderBy:sort_column:sort_reverse'>\n              <td>\n                <profile-pic owner='bounty.owner'></profile-pic>\n              </td>\n              <td>\n                <profile-name owner='bounty.owner'></profile-name>\n              </td>\n              <td>\n                <div>\n                  <strong>{{ bounty.amount | dollars }}</strong>\n                </div>\n              </td>\n              <td>\n                <div class='text-muted'>{{ bounty.created_at | date }}</div>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </team-view>\n</div>")}]),angular.module("app").controller("IssueBackersController",["$scope","$routeParams","$pageTitle","Issue","Bounties",function($scope,$routeParams,$pageTitle,Issue,Bounties){$scope.issue=Issue.get({id:$routeParams.id,include_tracker:!0,include_team:!0,include_counts:!0},function(issue){$scope.team=issue.team,$pageTitle.set("Backers",$scope.issue.title,$scope.issue.tracker.name)}),$scope.bounties=Bounties.get({issue_id:$routeParams.id,include_owner:!0,order:"+amount"}),$scope.sort_column="amount",$scope.sort_reverse=!0,$scope.sort_by=function(col){$scope.sort_column===col?$scope.sort_reverse=!$scope.sort_reverse:$scope.sort_column=col}}]),angular.module("app").controller("IssueDevelopersController",["$scope","$pageTitle","$api","$route","$routeParams","$location","$window","$analytics","$filter","$currency","$cart","$q","Issue","BountyClaim","Solution","DeveloperGoal","Bounties",function($scope,$pageTitle,$api,$route,$routeParams,$location,$window,$analytics,$filter,$currency,$cart,$q,Issue,BountyClaim,Solution,DeveloperGoal){var issue_id=parseInt($routeParams.id),page_data={initialize:function(){$scope.$watch("current_person",page_data.process_resources),page_data.refresh_issue(),page_data.refresh_bounty_claims(),page_data.refresh_solutions(),page_data.refresh_developer_goals(),page_data.refresh_issue_suggestions()},refresh_issue:function(){$scope.issue=Issue.get({id:issue_id,include_tracker:!0,include_counts:!0,include_team:!0,can_respond_to_claims:!0},function(issue){$scope.team=issue.team,$pageTitle.set("Developers",$scope.issue.title,$scope.issue.tracker.name),page_data.process_resources()})},refresh_bounty_claims:function(){return $scope.bounty_claims=BountyClaim.query({issue_id:issue_id,include_owner:!0,include_responses:!0}),$scope.bounty_claims.$promise.then(page_data.process_resources),$scope.bounty_claims.$promise},refresh_solutions:function(){return $scope.solutions=Solution.query({issue_id:issue_id,include_owner:!0}),$scope.solutions.$promise.then(page_data.process_resources),$scope.solutions.$promise},refresh_developer_goals:function(){return $scope.developer_goals=DeveloperGoal.query({issue_id:issue_id,include_owner:!0}),$scope.developer_goals.$promise.then(page_data.process_resources),$scope.developer_goals.$promise},refresh_issue_suggestions:function(){return $scope.issue_suggestions=$api.issue_suggestions.query({issue_id:issue_id}),$scope.issue_suggestions.$promise.then(page_da